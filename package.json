{
  "name": "riot-firebase-es6",
  "version": "1.0.0",
  "description": "riot and firebase es6 templ with rollup and karma.",
  "scripts": {
    "test": "karma start",
    "clean": "rm -rf public/dest && mkdir public/dest",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "preserve": "npm run test && npm run build",
