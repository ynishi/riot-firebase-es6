    "es6-shim": "^0.35.3",
    "expect.js": "^0.3.1",
    "firebase": "^4.1.3",
    "karma": "^1.7.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-rollup-preprocessor": "^4.0.0",
    "mocha": "^3.4.2",
    "phantomjs-prebuilt": "^2.1.14",
    "riot": "^3.6.1",
    "riot-router": "^0.9.7",
    "rollup": "^0.43.0",
    "rollup-plugin-buble": "^0.15.0",
